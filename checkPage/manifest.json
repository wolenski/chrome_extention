{
   "name": "checkPage",
   "description": "check page on my page-analyser",
   "version":"1.0",
   "manifest_version": 2,
   "permissions": [ "tabs", "http://*/", "https://*/" ],
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "options_page": "options.html",
   "background": {
	  "page" : "background.html"
  }	
}
